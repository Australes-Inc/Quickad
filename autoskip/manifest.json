{
  "manifest_version": 3,
  "name": "AutoSkip - YouTube Ad Skipper",
  "version": "1.0",
  "description": "Clique automatiquement sur le bouton Skip des publicit√©s YouTube",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "AutoSkip YouTube Ads"
  }
}