{
  "manifest_version": 3,
  "name": "Quickad",
  "version": "1.0.0",
  "description": "Accélerer la lecture des publicités YouTube pour les sauter plus rapidement.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quickad",
    "default_icon": {
      "16": "Quickad.png",
      "32": "Quickad.png",
      "48": "Quickad.png",
      "128": "Quickad.png"
    }
  },
  "icons": {
    "16": "Quickad.png",
    "32": "Quickad.png",
    "48": "Quickad.png",
    "128": "Quickad.png"
  }
}